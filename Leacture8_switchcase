// Switch cases and Introduction to functions
#include <iostream>

using namespace std;

int main()

{
  int n;

  cout<<"Enter the value of n under 5"<<endl;
  
  cin>>n;
  switch (n) {
    case 1 :
      cout<<"this is one"<<endl;
      break;
    case 2 :
      cout<<"this is two "<<endl;
      break;
    case 3:
      cout<<"this is three"<<endl;
      break;
    case 4:
      cout<<"this is four "<<endl;
      break;
    default:
      cout<<"this is the defacult "<<endl;
      
  }






}
